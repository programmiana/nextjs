{
  "compilerOptions": {
    "jsx": "preserve",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2017",
    "lib": ["ESNext", "dom"],
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "outDir": "out",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "isolatedModules": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "contracts"]
}
